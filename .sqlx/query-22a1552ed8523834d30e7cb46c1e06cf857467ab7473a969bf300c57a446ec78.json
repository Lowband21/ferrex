{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT r.id, r.name, r.description, r.is_system as \"is_system!\",\n                   EXTRACT(EPOCH FROM r.created_at)::BIGINT as \"created_at!\"\n            FROM roles r\n            INNER JOIN user_roles ur ON r.id = ur.role_id\n            WHERE ur.user_id = $1\n            ORDER BY r.name\n            ",
