  "query": "\n                    SELECT\n                        mr.id,\n                        mr.tmdb_id,\n                        mr.title,\n                        mr.theme_color,\n                        mr.batch_id,\n                        mf.id AS file_id,\n                        mf.library_id,\n                        mf.file_path,\n                        mf.filename,\n                        mf.file_size,\n                        mf.discovered_at AS file_discovered_at,\n                        mf.created_at AS file_created_at,\n                        mf.technical_metadata\n                    FROM movie_references mr\n                    JOIN media_files mf ON mr.file_id = mf.id\n                    WHERE mf.library_id = $1\n                    ORDER BY mr.title\n                    ",
