  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                u.id,\n                u.username,\n                u.is_active,\n                u.is_locked,\n                u.failed_login_attempts,\n                u.locked_until,\n                u.last_login,\n                uc.password_hash,\n                uc.pin_hash,\n                uc.pin_client_salt,\n                uc.pin_updated_at\n            FROM users u\n            INNER JOIN user_credentials uc ON u.id = uc.user_id\n            WHERE u.username = $1\n            ",
