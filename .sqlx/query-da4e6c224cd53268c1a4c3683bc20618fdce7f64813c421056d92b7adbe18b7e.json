{
  "db_name": "PostgreSQL",
  "query": "SELECT\n                mc.person_tmdb_id,\n                mc.credit_id,\n                mc.department,\n                mc.job,\n                p.name,\n                p.original_name,\n                p.profile_path,\n                p.gender,\n                p.known_for_department,\n                p.adult,\n                p.popularity,\n                p.imdb_id,\n                p.facebook_id,\n                p.instagram_id,\n                p.twitter_id,\n                p.wikidata_id,\n                p.tiktok_id,\n                p.youtube_id,\n                COALESCE(alias_data.aliases, ARRAY[]::TEXT[]) AS \"aliases!: Vec<String>\"\n            FROM movie_crew mc\n            JOIN persons p ON p.tmdb_id = mc.person_tmdb_id\n            LEFT JOIN (\n                SELECT tmdb_id, ARRAY_AGG(alias ORDER BY alias) AS aliases\n                FROM person_aliases\n                GROUP BY tmdb_id\n            ) alias_data ON alias_data.tmdb_id = mc.person_tmdb_id\n            WHERE mc.movie_id = $1",
