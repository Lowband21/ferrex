{
  "db_name": "PostgreSQL",
  "query": "SELECT\n                ec.person_tmdb_id,\n                ec.credit_id,\n                ec.department,\n                ec.job,\n                p.id,\n                p.name,\n                p.original_name,\n                p.profile_path,\n                p.gender,\n                p.known_for_department,\n                p.adult,\n                p.popularity,\n                p.imdb_id,\n                p.facebook_id,\n                p.instagram_id,\n                p.twitter_id,\n                p.wikidata_id,\n                p.tiktok_id,\n                p.youtube_id,\n                COALESCE(alias_data.aliases, ARRAY[]::TEXT[]) AS \"aliases!: Vec<String>\"\n            FROM episode_crew ec\n            JOIN persons p ON p.id = ec.person_id\n            LEFT JOIN (\n                SELECT person_id, ARRAY_AGG(alias ORDER BY alias) AS aliases\n                FROM person_aliases\n                GROUP BY person_id\n            ) alias_data ON alias_data.person_id = p.id\n            WHERE ec.episode_id = $1\n        ",
